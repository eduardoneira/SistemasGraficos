displace = [-0.5124, -0.0278];
x_max = 0.5173;
y_max = 0.0782;

% x = x - (1 + displace(1))*ones(length(x),1);
% y = y - (1 + displace(2))*ones(length(y),1);

control_points = [0.00492, 0;
                0.00492, 0.03221;
                -0.05275, 0.05038;
                -0.10021, 0.05038;
                -0.17913, 0.05038;
                -0.25522, 0.02035;
                -0.33203, 0.00222;
                -0.38001, -0.00911;
                -0.43056, -0.00226;
                -0.47986, -0.00226;
                -0.50817, -0.00226;
                -0.51234, -0.02770;
                -0.51234, 0];
            

for i = 1:size(control_points, 1)
    control_points(i,:) = control_points(i,:) - displace;
end

control_points = [1/x_max*control_points(:,1), 1/y_max*control_points(:,2)];
% x = 1/x_max*x;
% y = 1/y_max*y;
            
figure, hold on
plot(coords(:,1), coords(:,2))
plot(control_points(:,1), control_points(:,2), 'or')